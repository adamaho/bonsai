{
  "name": "bonsai",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\" && turbo run format",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@6.30.1"
}